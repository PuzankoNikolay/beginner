komanda = input().split()
familia = []
index = []
while komanda != ['Stop']:
    if komanda[2] == 'фамилию':
        familia.append(komanda[3])
        itog = familia[:]
        index.append(1)
        komanda = input().split()
    elif komanda[2] == 'индексу':
        t = 0
        for i in range(len(index)):
            t += 1
            if int(komanda[3]) == t:
                index[i] = 0
            else:
                print('В списке нет такого индекса')
        komanda = input().split()
    elif komanda[2] == 'фамилии':
        if komanda[3] in itog:
            itog.remove(komanda[3])
            komanda = input().split()
        else:
            print('В списке нет такой фамилии')
            komanda = input().split()
print(familia)
print(index)
print(itog)
