a = input().split()
for i in range(len(a)):
    if a[i] == a[0]:
        print('Предшественник: Нет')
        print('Текущая мумь:', a[0])
        print('Следующая мумь:', a[1])
    if a[i] == a[-1]:
        print('Предшественник:', a[i - 1])
        print('Текущая мумь:', a[i])
        print('Следующая мумь: нет')
    print('Предшественник:', a[i - 1])
    print('Текущая мумь:', a[i])
    print('Следующая мумь:', a[i + 1])


